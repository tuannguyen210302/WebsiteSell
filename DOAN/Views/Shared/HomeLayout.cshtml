    <link rel="stylesheet" href="~/assets/client/css/jquery.fancybox.min.css">
    <!-- Themify Icons -->
    <link rel="stylesheet" href="~/assets/client/css/themify-icons.css">
    <!-- Nice Select CSS -->
    <link rel="stylesheet" href="~/assets/client/css/niceselect.css">
    <!-- Animate CSS -->
    <link rel="stylesheet" href="~/assets/client/css/animate.css">
    <!-- Flex Slider CSS -->
    <link rel="stylesheet" href="~/assets/client/css/flex-slider.min.css">
    <!-- Owl Carousel -->
    <link rel="stylesheet" href="~/assets/client/css/owl-carousel.css">
