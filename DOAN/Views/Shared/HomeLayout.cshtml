    <!-- Nice Select JS -->
    <script src="~/assets/client/js/nicesellect.js"></script>
    <!-- Flex Slider JS -->
    <script src="~/assets/client/js/flex-slider.js"></script>
    <!-- ScrollUp JS -->
    <script src="~/assets/client/js/scrollup.js"></script>
    <!-- Onepage Nav JS -->
    <script src="~/assets/client/js/onepage-nav.min.js"></script>
    <!-- Easing JS -->
    <script src="~/assets/client/js/easing.js"></script>
    <!-- Active JS -->
    <script src="~/assets/client/js/active.js"></script>
