    <!-- Eshop StyleSheet -->
    <link rel="stylesheet" href="~/assets/client/css/reset.css">
    <link rel="stylesheet" href="~/assets/client/style.css">
    <link rel="stylesheet" href="~/assets/client/css/responsive.css">



</head>
<body class="js">

    <!-- Preloader -->
    <!--<div class="preloader">
        <div class="preloader-inner">
            <div class="preloader-icon">
                <span></span>
                <span></span>
            </div>
        </div>
    </div>-->
    <!-- End Preloader -->
    <!-- Header -->
    <header class="header shop">
        <!-- Topbar -->
        @Html.Partial("~/Views/Home/HeaderTopPartial.cshtml")



        @Html.Action("MenuPartial", "Home")



    </header>
    <!--/ End Header -->


    <section id="showcase">
        <div id="carousel" class="carousel slide" data-ride="carousel">
            <ol class="carousel-indicators">
                <li data-target="#carousel" data-slide-to="0" class="active"></li>
                <li data-target="#carousel" data-slide-to="1"></li>
                <li data-target="#carousel" data-slide-to="2"></li>
                <li data-target="#carousel" data-slide-to="3"></li>
            </ol>
            <div class="carousel-inner" role="listbox">
                <div class="carousel-item active">
                    <img src="@Url.Content("~/assets/client/hinhsp/slide/slide1.jpg")" alt="Slide #1" class="d-block img-fluid">
                    <div class="carousel-caption">

                    </div>
                </div>
                <div class="carousel-item">
                    <img src="@Url.Content("~/assets/client/hinhsp/slide/slide2.jpg")" alt="Slide #2" class="d-block img-fluid">
                    <div class="carousel-caption">

                    </div>
                </div>
                <div class="carousel-item">
                    <img src="@Url.Content("~/assets/client/hinhsp/slide/slide3.jpg")" alt="Slide #3" class="d-block img-fluid">
                    <div class="carousel-caption">

                    </div>
                </div>
                <div class="carousel-item">
                    <img src="@Url.Content("~/assets/client/hinhsp/slide/slide4.jpg")" alt="Slide #4" class="d-block img-fluid">
                    <div class="carousel-caption">

                    </div>
                </div>
            </div>
            <a href="#carousel" class="carousel-control-prev" role="button" data-slide="prev">
                <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                <span class="sr-only">Previous</span>
            </a>
            <a href="#carousel" class="carousel-control-next" role="button" data-slide="next">
                <span class="carousel-control-next-icon" aria-hidden="true"></span>
                <span class="sr-only">Next</span>
            </a>
        </div>
    </section>




    @RenderBody()



    <!-- Start Shop Services Area -->
    <section class="shop-services section home">
        <div class="container">
            <div class="row">
                <div class="col-lg-3 col-md-6 col-12">
                    <!-- Start Single Service -->
                    <div class="single-service">
                        <i class="ti-rocket"></i>
                        <h4>Nhanh chóng</h4>
                        <p>Chi phí rẻ</p>
                    </div>
                    <!-- End Single Service -->
                </div>
                <div class="col-lg-3 col-md-6 col-12">
                    <!-- Start Single Service -->
                    <div class="single-service">
                        <i class="ti-reload"></i>
                        <h4>Được phép kiểm tra</h4>
                        <p>Trả hàng tại chỗ</p>
                    </div>
                    <!-- End Single Service -->
                </div>
                <div class="col-lg-3 col-md-6 col-12">
                    <!-- Start Single Service -->
                    <div class="single-service">
                        <i class="ti-lock"></i>
                        <h4>Thanh toán bảo mật</h4>
                        <p>100% bảo mật</p>
                    </div>
                    <!-- End Single Service -->
                </div>
                <div class="col-lg-3 col-md-6 col-12">
                    <!-- Start Single Service -->
                    <div class="single-service">
                        <i class="ti-tag"></i>
                        <h4>Giá tốt nhất</h4>
                        <p>Đảm bảo chất lượng</p>
                    </div>
                    <!-- End Single Service -->
                </div>
            </div>
        </div>
    </section>
    <!-- End Shop Services Area -->
    <!-- Start Shop Newsletter  -->
    <section class="shop-newsletter section">
        <div class="container">
            <div class="inner-top">
                <div class="row">
                    <div class="col-lg-8 offset-lg-2 col-12">
                        <!-- Start Newsletter Inner -->
                        
                        <!-- End Newsletter Inner -->
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- End Shop Newsletter -->
    
    <!-- Modal end -->


    @Html.Partial("~/Views/Home/FooterPartial.cshtml")

    <!-- Jquery -->
    <script src="~/assets/client/js/jquery.min.js"></script>
    <script src="~/assets/client/js/jquery-migrate-3.0.0.js"></script>
    <script src="~/assets/client/js/jquery-ui.min.js"></script>
    <script src="~/Scripts/jquery.unobtrusive-ajax.min.js"></script>
    <!-- Popper JS -->
    <script src="~/assets/client/js/popper.min.js"></script>
    <!-- Bootstrap JS -->
    <script src="~/assets/client/js/bootstrap.min.js"></script>
    <!-- Color JS -->
    <script src="~/assets/client/js/colors.js"></script>
    <!-- Slicknav JS -->
    <script src="~/assets/client/js/slicknav.min.js"></script>
    <!-- Owl Carousel JS -->
    <script src="~/assets/client/js/owl-carousel.js"></script>
    <!-- Magnific Popup JS -->
    <script src="~/assets/client/js/magnific-popup.js"></script>
    <!-- Waypoints JS -->
    <script src="~/assets/client/js/waypoints.min.js"></script>
    <!-- Countdown JS -->
