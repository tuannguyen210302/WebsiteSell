    <!-- Title Tag  -->
    <title>Sociolla</title>
    <!-- Favicon -->
    <link rel="icon" type="image/png" href="~/assets/client/images/favicon.png">
    <!-- Web Font -->
    
    <link href="https://fonts.googleapis.com/css?family=Roboto&display=swap" rel="stylesheet">
